        <Line X1="43" Y1="26" X2="43" Y2="36" Stroke="Black"/>
        <Line X1="18" Y1="42" X2="28" Y2="42" Stroke="Black"/>
        <Line X1="23" Y1="37" X2="23" Y2="47" Stroke="Black"/>
    </Canvas>
    <Canvas x:Key="cmdParallelSegmentIcon">
        <Line X1="10" Y1="15" X2="50" Y2="15" Stroke="Gray"/>
        <Line X1="15" Y1="15" X2="30" Y2="50" Stroke="Black"/>
        <Line X1="33" Y1="21" X2="42" Y2="43" Stroke="Red"/>
