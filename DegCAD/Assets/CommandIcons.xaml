        <TextBlock Text="A" Foreground="Red" FontSize="16" Canvas.Left="35" Canvas.Top="32"/>
    </Canvas>
    <Canvas x:Key="cmdPointProjectionsIcon">
        <Line X1="10" Y1="30" X2="50" Y2="30" Stroke="Gray"/>
        <Line X1="25" Y1="15" X2="35" Y2="15" Stroke="Red"/>
        <Line X1="30" Y1="10" X2="30" Y2="20" Stroke="Red"/>
        <Line X1="25" Y1="45" X2="35" Y2="45" Stroke="Red"/>
        <Line X1="30" Y1="40" X2="30" Y2="50" Stroke="Red"/>
    </Canvas>
    <Canvas x:Key="cmdPointAddProjectionIcon">
