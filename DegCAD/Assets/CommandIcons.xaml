        <Line X1="30" Y1="22" X2="33" Y2="28" Stroke="Red"/>
    </Canvas>
    <Canvas x:Key="cmdPointInputIcon">
        <TextBlock Text="A[x,y,z]" FontSize="16" Canvas.Top="8" Canvas.Left="6" Foreground="Black"/>
        <TextBlock Text="B[x,y,z]" FontSize="16" Canvas.Top="28" Canvas.Left="6" Foreground="Black"/>
    </Canvas>
    <Canvas x:Key="cmdPointInputIcon2D">
        <TextBlock Text="A[x,y]" FontSize="16" Canvas.Top="8" Canvas.Left="6" Foreground="Black"/>
        <TextBlock Text="B[x,y]" FontSize="16" Canvas.Top="28" Canvas.Left="6" Foreground="Black"/>
    </Canvas>
    <Canvas x:Key="cmdMiddleIcon">
        <Line X1="5" Y1="20" X2="15" Y2="20" Stroke="Black"/>
        <Line X1="10" Y1="15" X2="10" Y2="25" Stroke="Black"/>
        <Line X1="45" Y1="40" X2="55" Y2="40" Stroke="Black"/>
        <Line X1="50" Y1="35" X2="50" Y2="45" Stroke="Black"/>
        <Line X1="25" Y1="30" X2="35" Y2="30" Stroke="Red"/>
        <Line X1="30" Y1="25" X2="30" Y2="35" Stroke="Red"/>
        <TextBlock Text="S" Foreground="Red" Canvas.Left="32" Canvas.Top="29"/>
    </Canvas>
    <Canvas x:Key="cmdLineProjectionIcon">
        <Line X1="10" Y1="15" X2="50" Y2="15" Stroke="Gray"/>
        <Line X1="20" Y1="15" X2="40" Y2="50" Stroke="Red"/>
    </Canvas>
    <Canvas x:Key="cmdLineProjectionsIcon">
        <Line X1="10" Y1="10" X2="40" Y2="30" Stroke="Red"/>
        <Line X1="20" Y1="30" X2="50" Y2="50" Stroke="Red"/>
        <Line X1="10" Y1="30" X2="50" Y2="30" Stroke="Gray"/>
    </Canvas>
    <Canvas x:Key="cmdPerpendicularLineIcon">
        <Line X1="10" Y1="15" X2="50" Y2="15" Stroke="Gray"/>
        <Line X1="20" Y1="15" X2="40" Y2="50" Stroke="Black"/>
        <Line X1="10" Y1="50" X2="50" Y2="27" Stroke="Red"/>
        <Line X1="38" Y1="31" X2="48" Y2="31" Stroke="Black"/>
        <Line X1="43" Y1="26" X2="43" Y2="36" Stroke="Black"/>
    </Canvas>
    <Canvas x:Key="cmdParallelLineIcon">
        <Line X1="10" Y1="15" X2="50" Y2="15" Stroke="Gray"/>
        <Line X1="15" Y1="15" X2="30" Y2="50" Stroke="Black"/>
