        <Line X1="30" Y1="20" X2="30" Y2="40" Stroke="{DynamicResource fg}" StrokeThickness="1.4"/>
        <TextBlock Text="+A" Foreground="{DynamicResource red}" FontSize="16" Canvas.Left="30" Canvas.Top="32" FontWeight="DemiBold"/>
    </Canvas>
    <Canvas x:Key="cmdInputLengthIcon">
        <Line X1="10" Y1="40" X2="50" Y2="40" Stroke="{DynamicResource fg}" StrokeThickness="1.4"/>
