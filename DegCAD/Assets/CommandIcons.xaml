    <Canvas x:Key="cmdCircleIcon">
        <Ellipse Canvas.Top="10" Canvas.Left="10" Width="40" Height="40" Stroke="Red"/>
        <Line X1="25" Y1="30" X2="35" Y2="30" Stroke="Black"/>
        <Line X1="30" Y1="25" X2="30" Y2="35" Stroke="Black"/>
        <Line X1="37" Y1="15" X2="47" Y2="15" Stroke="Black"/>
        <Line X1="42" Y1="10" X2="42" Y2="20" Stroke="Black"/>
    </Canvas>
    <Canvas x:Key="cmdArcIcon">
        <Path Stroke="Red" StrokeThickness="1" Data="M 20 10 Q 45 15 50 40" />
        <Line X1="20" Y1="35" X2="20" Y2="45" Stroke="Black"/>
        <Line X1="15" Y1="40" X2="25" Y2="40" Stroke="Black"/>
        <Line X1="20" Y1="5" X2="20" Y2="15" Stroke="Black"/>
        <Line X1="15" Y1="10" X2="25" Y2="10" Stroke="Black"/>
        <Line X1="50" Y1="35" X2="50" Y2="45" Stroke="Black"/>
        <Line X1="45" Y1="40" X2="55" Y2="40" Stroke="Black"/>
    </Canvas>
    <Canvas x:Key="cmdEllipseIcon">
        <Ellipse Canvas.Top="20" Canvas.Left="10" Width="40" Height="20" Stroke="Red"/>
        <Line X1="10" Y1="30" X2="50" Y2="30" Stroke="Black"/>
        <Line X1="30" Y1="20" X2="30" Y2="40" Stroke="Black"/>
    </Canvas>
    <Canvas x:Key="cmdParabolaIcon">
        <Path Data="M 15 15 Q 30 75 45 15" Stroke="Red"/>
        <Line X1="25" Y1="30" X2="35" Y2="30" Stroke="Black"/>
        <Line X1="30" Y1="25" X2="30" Y2="35" Stroke="Black"/>
        <Line X1="30" Y1="40" X2="30" Y2="50" Stroke="Black"/>
    </Canvas>
