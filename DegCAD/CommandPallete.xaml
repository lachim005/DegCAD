<UserControl x:Class="DegCAD.CommandPallete"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DegCAD"
             mc:Ignorable="d"
             Foreground="{DynamicResource fg}"
             d:DesignHeight="100" d:DesignWidth="800">
    <Grid>
        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Disabled" PreviewMouseWheel="ScrollViewer_MouseWheel" Background="Transparent">
            <StackPanel x:Name="cmdButtons" Orientation="Horizontal" Height="100"/>
        </ScrollViewer>
    </Grid>
</UserControl>
