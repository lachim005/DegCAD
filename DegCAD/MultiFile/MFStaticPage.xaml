<UserControl x:Class="DegCAD.MultiFile.MFStaticPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DegCAD.MultiFile"
             mc:Ignorable="d" 
             Loaded="OnLoaded"
             SizeChanged="OnSizeChanged"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Border x:Name="paper" Background="Transparent" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Canvas x:Name="pageItems" ClipToBounds="True"/>
        </Border>
        <Rectangle Fill="Transparent"/>
    </Grid>
</UserControl>
