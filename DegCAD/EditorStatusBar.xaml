<UserControl x:Class="DegCAD.EditorStatusBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DegCAD"
             mc:Ignorable="d" 
             d:DesignHeight="23" d:DesignWidth="800">
    <StatusBar Grid.Row="1" Grid.ColumnSpan="3">
        <StatusBarItem>
            <TextBlock x:Name="cmdName" Visibility="Collapsed"/>
        </StatusBarItem>
        <Separator/>
        <StatusBarItem>
            <TextBlock x:Name="cmdHelp" Visibility="Collapsed"/>
        </StatusBarItem>
    </StatusBar>
</UserControl>
