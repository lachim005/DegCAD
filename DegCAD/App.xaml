<Application x:Class="DegCAD.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DegCAD"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <local:SkinResourceDictionary LightSource="Assets/ColorsLight.xaml" DarkSource="Assets/ColorsDark.xaml"/>
                <ResourceDictionary Source="Assets/AppStyles.xaml"/>
                <ResourceDictionary Source="Assets/CommandIcons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
